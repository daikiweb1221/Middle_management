d>\n                    </template>\n                    <v-date-picker\n                      v-model=\"subordinate.birthday\"\n                      :active-picker.sync=\"activePicker\"\n                      :max=\"\n                        new Date(\n                          Date.now() - new Date().getTimezoneOffset() * 60000\n                        )\n                          .toISOString()\n                          .substr(0, 10)\n                      \"\n                      min=\"1950-01-01\"\n                      @change=\"save\"\n                    ></v-date-picker>\n                  </v-menu>\n                </ValidationProvider>\n              </v-col>\n            </v-row>\n          </v-form>\n        </v-container>\n      </v-card-text>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"blue darken-1\" text @click=\"handleCloseModal\">\n          閉じる\n        </v-btn>\n        <v-btn\n          color=\"blue darken-1\"\n          text\n          @click=\"handleSubmit(handleUpdateSubordinate)\"\n        >\n          更新する\n        </v-btn>\n      </v-card-actions>\n    </ValidationObserver>\n  </v-card>\n</template>\n\n<script>\nexport default {\n  name: \"SubordinateEditModal\",\n\n  data() {\n    return {\n      activePicker: null,\n      menu: false,\n    };\n  },\n\n  props: {\n    dialog: {\n      type: Boolean,\n    },\n    subordinate: {\n      type: Object,\n      required: true,\n      id: {\n        type: Number,\n        required: true,\n      },\n      name: {\n        type: String,\n        required: true,\n      },\n      email: {\n        type: String,\n      },\n      birthday: {\n        type: Date,\n      },\n    },\n  },\n\n  watch: {\n    menu(val) {\n      val && setTimeout(() => (this.activePicker = \"YEAR\"));\n    },\n  },\n\n  methods: {\n    handleCloseModal() {\n      this.$emit(\"close-modal\");\n    },\n    handleUpdateSubordinate() {\n      this.$emit(\"update-subordinate\", this.subordinate);\n    },\n    save(date) {\n      this.$refs.menu.save(date);\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module"}