lass=\"form-group\">\n                <ValidationProvider v-slot=\"{ errors }\" rules=\"email\">\n                  <label for=\"email\">メールアドレス</label>\n                  <input\n                    type=\"email\"\n                    class=\"form-control\"\n                    id=\"email\"\n                    v-model=\"subordinate.email\"\n                  />\n                  <span class=\"text-danger\">{{ errors[0] }}</span>\n                </ValidationProvider>\n              </div>\n              <div class=\"form-group\">\n                <label for=\"birthday\">誕生日</label>\n                <input\n                  type=\"date\"\n                  class=\"form-control\"\n                  id=\"birthday\"\n                  v-model=\"subordinate.birthday\"\n                />\n              </div>\n              <div class=\"d-flex justify-content-between\">\n                <button\n                  class=\"btn btn-success\"\n                  @click=\"handleSubmit(handleUpdateSubordinate)\"\n                >\n                  更新する\n                </button>\n                <button class=\"btn btn-secondary\" @click=\"handleCloseModal\">\n                  閉じる\n                </button>\n              </div>\n            </ValidationObserver>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"modal-backdrop show\"></div>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"SubordinateEditModal\",\n  props: {\n    subordinate: {\n      type: Object,\n      required: true,\n      id: {\n        type: Number,\n        required: true,\n      },\n      name: {\n        type: String,\n        required: true,\n      },\n      email: {\n        type: String,\n      },\n      birthday: {\n        type: Date,\n      },\n    },\n  },\n  methods: {\n    handleCloseModal() {\n      this.$emit(\"close-modal\");\n    },\n    handleUpdateSubordinate() {\n      this.$emit(\"update-subordinate\", this.subordinate);\n    },\n  },\n};\n</script>\n\n<style scoped>\n.modal {\n  display: block;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}