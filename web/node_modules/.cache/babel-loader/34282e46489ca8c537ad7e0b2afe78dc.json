 </v-btn>\n            <v-btn\n              color=\"blue darken-1\"\n              text\n              @click=\"handleSubmit(handleCreateSubordinate)\"\n            >\n              追加する\n            </v-btn>\n          </v-card-actions>\n        </ValidationObserver>\n      </v-card>\n    </v-dialog>\n  </v-row>\n</template>\n\n<script>\nexport default {\n  name: \"SubordinateCreateModal\",\n  data() {\n    return {\n      subordinate: {\n        name: \"\",\n        email: \"\",\n        birthday: \"\",\n      },\n      dialog: false,\n    };\n  },\n  methods: {\n    clear() {\n      this.subordinate.name = \"\";\n      this.subordinate.email = \"\";\n      this.subordinate.birthday = \"\";\n      this.$refs.observer.reset();\n    },\n    handleCreateSubordinate() {\n      this.$emit(\"create-subordinate\", this.subordinate);\n      this.dialog = false;\n      this.clear();\n    },\n    closeForm() {\n      this.dialog = false;\n      this.clear();\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module"}