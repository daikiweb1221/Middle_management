{"ast":null,"code":"export default {\n  name: \"SubordinateCreateModal\",\n  data: function data() {\n    return {\n      subordinate: {\n        name: \"\",\n        email: \"\",\n        birthday: \"\"\n      },\n      dialog: false,\n      activePicker: null,\n      menu: false\n    };\n  },\n  watch: {\n    menu: function menu(val) {\n      var _this = this;\n      val && setTimeout(function () {\n        return _this.activePicker = \"YEAR\";\n      });\n    }\n  },\n  methods: {\n    clear: function clear() {\n      this.subordinate.name = \"\";\n      this.subordinate.email = \"\";\n      this.subordinate.birthday = \"\";\n      this.$refs.observer.reset();\n    },\n    handleCreateSubordinate: function handleCreateSubordinate() {\n      this.$emit(\"create-subordinate\", this.subordinate);\n      this.dialog = false;\n      this.clear();\n    },\n    closeForm: function closeForm() {\n      this.dialog = false;\n      this.clear();\n    },\n    save: function save(date) {\n      this.$refs.menu.save(date);\n    }\n  }\n};","map":{"version":3,"mappings":"AA0HA;EACAA;EACAC;IACA;MACAC;QACAF;QACAG;QACAC;MACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAD;MAAA;MACAE;QAAA;MAAA;IACA;EACA;EACAC;IACAC;MACA;MACA;MACA;MACA;IACA;IACAC;MACA;MACA;MACA;IACA;IACAC;MACA;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["name","data","subordinate","email","birthday","dialog","activePicker","menu","watch","val","methods","clear","handleCreateSubordinate","closeForm","save"],"sourceRoot":"app/javascript/pages/components","sources":["SubordinateCreateModal.vue"],"sourcesContent":["<template>\n  <v-row justify=\"center\">\n    <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\n      <template v-slot:activator=\"{ on, attrs }\">\n        <v-btn class=\"mx-2\" fab dark color=\"primary\" v-bind=\"attrs\" v-on=\"on\">\n          <v-icon dark> mdi-plus </v-icon>\n        </v-btn>\n      </template>\n      <v-card>\n        <ValidationObserver v-slot=\"{ handleSubmit }\" ref=\"observer\">\n          <v-card-title>\n            <span class=\"text-h5\">Subordinate Profile</span>\n          </v-card-title>\n          <v-card-text>\n            <v-container>\n              <v-form>\n                <v-row>\n                  <v-col cols=\"12\">\n                    <ValidationProvider\n                      v-slot=\"{ errors }\"\n                      rules=\"required|max:20\"\n                      name=\"名前\"\n                    >\n                      <v-text-field\n                        label=\"名前\"\n                        type=\"text\"\n                        id=\"name\"\n                        name=\"name\"\n                        clearable\n                        v-model=\"subordinate.name\"\n                        :error-messages=\"errors\"\n                      ></v-text-field>\n                    </ValidationProvider>\n                  </v-col>\n                  <v-col cols=\"12\">\n                    <ValidationProvider\n                      v-slot=\"{ errors }\"\n                      rules=\"email\"\n                      name=\"メールアドレス\"\n                    >\n                      <v-text-field\n                        label=\"メールアドレス\"\n                        type=\"email\"\n                        id=\"email\"\n                        name=\"email\"\n                        clearable\n                        v-model=\"subordinate.email\"\n                        :error-messages=\"errors\"\n                      ></v-text-field>\n                    </ValidationProvider>\n                  </v-col>\n                  <v-col cols=\"12\">\n                    <ValidationProvider v-slot=\"{ errors }\" name=\"誕生日\">\n                      <!-- <v-text-field\n                        label=\"誕生日\"\n                        type=\"date\"\n                        id=\"birthday\"\n                        name=\"birthday\"\n                        clearable\n                        v-model=\"subordinate.birthday\"\n                        :error-messages=\"errors\"\n                      ></v-text-field> -->\n                      <v-menu\n                        ref=\"menu\"\n                        v-model=\"menu\"\n                        :close-on-content-click=\"false\"\n                        transition=\"scale-transition\"\n                        offset-y\n                        min-width=\"auto\"\n                      >\n                        <template v-slot:activator=\"{ on, attrs }\">\n                          <v-text-field\n                            :error-messages=\"errors\"\n                            v-model=\"subordinate.birthday\"\n                            label=\"誕生日\"\n                            prepend-icon=\"mdi-calendar\"\n                            readonly\n                            v-bind=\"attrs\"\n                            v-on=\"on\"\n                          ></v-text-field>\n                        </template>\n                        <v-date-picker\n                          v-model=\"date\"\n                          :active-picker.sync=\"activePicker\"\n                          :max=\"\n                            new Date(\n                              Date.now() -\n                                new Date().getTimezoneOffset() * 60000\n                            )\n                              .toISOString()\n                              .substr(0, 10)\n                          \"\n                          min=\"1950-01-01\"\n                          @change=\"save\"\n                        ></v-date-picker>\n                      </v-menu>\n                    </ValidationProvider>\n                  </v-col>\n                </v-row>\n              </v-form>\n            </v-container>\n          </v-card-text>\n          <v-card-actions>\n            <v-spacer></v-spacer>\n            <v-btn color=\"blue darken-1\" text @click=\"closeForm()\">\n              閉じる\n            </v-btn>\n            <v-btn\n              color=\"blue darken-1\"\n              text\n              @click=\"handleSubmit(handleCreateSubordinate)\"\n            >\n              追加する\n            </v-btn>\n          </v-card-actions>\n        </ValidationObserver>\n      </v-card>\n    </v-dialog>\n  </v-row>\n</template>\n\n<script>\nexport default {\n  name: \"SubordinateCreateModal\",\n  data() {\n    return {\n      subordinate: {\n        name: \"\",\n        email: \"\",\n        birthday: \"\",\n      },\n      dialog: false,\n      activePicker: null,\n      menu: false,\n    };\n  },\n  watch: {\n    menu(val) {\n      val && setTimeout(() => (this.activePicker = \"YEAR\"));\n    },\n  },\n  methods: {\n    clear() {\n      this.subordinate.name = \"\";\n      this.subordinate.email = \"\";\n      this.subordinate.birthday = \"\";\n      this.$refs.observer.reset();\n    },\n    handleCreateSubordinate() {\n      this.$emit(\"create-subordinate\", this.subordinate);\n      this.dialog = false;\n      this.clear();\n    },\n    closeForm() {\n      this.dialog = false;\n      this.clear();\n    },\n    save(date) {\n      this.$refs.menu.save(date);\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module"}